{
  "name": "hapi",
  "description": "HTTP Server framework",
  "homepage": "http://hapijs.com",
  "version": "0.15.0",
  "author": "Eran Hammer <eran@hueniverse.com> (http://hueniverse.com)",
  "contributors": [
    "Van Nguyen <the.gol.effect@gmail.com>",
